<div class="container container_grey emphasize-dark curved_border">
<div class="row-fluid col-md-offset-1 col-md-5">

  <p class="font_trebuchet_blk_20">Editing User Details</p>
  <%= render 'form' %>
  <% if current_user and current_user.admin %>
      <%= link_to 'Show', @user %> |
      <%= link_to 'Back', users_path %>
  <% else %>
      <%= link_to 'Back', :controller => "users", :action => "welcome", :notice => "Edited successfully" %>
  <% end %>

</div>
</div>